<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.gnome.olc" path="/org/gnome/OpenLightingConsole/" gettext-domain="olc">
    <key name="universe" type="i">
      <default>0</default>
      <summary>Universe number</summary>
      <description></description>
    </key>
    <key name="percent" type="b">
      <default>false</default>
      <summary>Display in percent</summary>
      <description>Display levels in percent state.</description>
    </key>
    <key name="osc-host" type="s">
      <default>"10.0.0.3"</default>
      <summary>OSC client's IP address</summary>
      <description></description>
    </key>
    <key name="osc-server-port" type="i">
      <default>7000</default>
      <summary>OSC server port</summary>
      <description>Port use by OSC server to listen</description>
    </key>
    <key name="osc-client-port" type="i">
      <default>9000</default>
      <summary>OSC client port</summary>
      <description>Port use by OSC to send data to the client</description>
    </key>
  </schema>
</schemalist>

